{
  "name": "huff-code",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build -c=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:fly:check": "npx prettier --check .",
    "prettier:fly:fix": "npx prettier --write .",
    "start:fly": "npx -p @angular/cli ng serve",
    "build:fly": "npx -p @angular/cli ng build",
    "watch:fly": "npx -p @angular/cli ng build --watch --configuration development",
    "test:fly": "npx -p @angular/cli ng test",
    "test:fly:headless": "npx -p @angular/cli ng test --watch=false --browsers=ChromeHeadless",
    "lint:fly:check": "npx -p @angular/cli ng lint",
    "lint:fly:fix": "npx -p @angular/cli ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/file-input": "^16.0.1",
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "puppeteer": "^21.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript-collections": "^1.3.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.1.3"
  }
}
